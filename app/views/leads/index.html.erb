<h1>Listing leads</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Day phone</th>
      <th>Evening phone</th>
      <th>Fax</th>
      <th>Email</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Household income</th>
      <th>Date received</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.first_name %></td>
        <td><%= lead.last_name %></td>
        <td><%= lead.day_phone %></td>
        <td><%= lead.evening_phone %></td>
        <td><%= lead.fax %></td>
        <td><%= lead.email %></td>
        <td><%= lead.address1 %></td>
        <td><%= lead.address2 %></td>
        <td><%= lead.city %></td>
        <td><%= lead.state %></td>
        <td><%= lead.zip %></td>
        <td><%= lead.household_income %></td>
        <td><%= lead.date_received %></td>
        <td><%= lead.type %></td>
        <td><%= link_to 'Show', lead %></td>
        <td><%= link_to 'Edit', edit_lead_path(lead) %></td>
        <td><%= link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lead', new_lead_path %>
